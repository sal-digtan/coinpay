{
  "expo": {
    "name": "coinpay",
    "slug": "coinpay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/coinpayLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/coinpayLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1D2869"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/coinpayLogo.png",
        "backgroundColor": "#1D2869"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/coinpayLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins.ttf"
          ]
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/Poppins/Poppins_400Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}